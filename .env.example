# Server Configuration
PORT=3000

# RTSP Stream Configuration
# Replace with your actual RTSP URL
RTSP_URL=rtsp://username:password@camera_ip:port

# Capture Settings (in milliseconds)
# How often to capture images from the stream
CAPTURE_INTERVAL=5000

# Basic Authentication
# Set AUTH_ENABLED=false to disable authentication
AUTH_ENABLED=true
AUTH_USERNAME=admin
AUTH_PASSWORD=your_secure_password